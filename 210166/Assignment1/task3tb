#! /usr/bin/vvp
:ivl_version "10.3 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x5557a0d320f0 .scope module, "tb" "tb" 2 2;
 .timescale 0 0;
v0x5557a0d68250_0 .var "T", 4 0;
v0x5557a0d68310_0 .net "fsm", 1 0, v0x5557a0d56f20_0;  1 drivers
S_0x5557a0d56d30 .scope module, "heat0" "heating_system" 2 5, 3 1 0, S_0x5557a0d320f0;
 .timescale 0 0;
    .port_info 0 /INPUT 5 "temp"
    .port_info 1 /OUTPUT 2 "C"
v0x5557a0d56f20_0 .var "C", 1 0;
v0x5557a0d68110_0 .net "temp", 4 0, v0x5557a0d68250_0;  1 drivers
E_0x5557a0d52520 .event edge, v0x5557a0d68110_0;
    .scope S_0x5557a0d56d30;
T_0 ;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x5557a0d56f20_0, 0, 2;
    %end;
    .thread T_0;
    .scope S_0x5557a0d56d30;
T_1 ;
    %wait E_0x5557a0d52520;
    %load/vec4 v0x5557a0d56f20_0;
    %cmpi/e 2, 0, 2;
    %jmp/0xz  T_1.0, 4;
    %pushi/vec4 26, 0, 5;
    %load/vec4 v0x5557a0d68110_0;
    %cmp/u;
    %jmp/0xz  T_1.2, 5;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x5557a0d56f20_0, 0, 2;
    %jmp T_1.3;
T_1.2 ;
    %pushi/vec4 20, 0, 5;
    %load/vec4 v0x5557a0d68110_0;
    %cmp/u;
    %jmp/0xz  T_1.4, 5;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x5557a0d56f20_0, 0, 2;
T_1.4 ;
T_1.3 ;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x5557a0d56f20_0;
    %cmpi/e 1, 0, 2;
    %jmp/0xz  T_1.6, 4;
    %load/vec4 v0x5557a0d68110_0;
    %cmpi/u 8, 0, 5;
    %jmp/0xz  T_1.8, 5;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x5557a0d56f20_0, 0, 2;
    %jmp T_1.9;
T_1.8 ;
    %load/vec4 v0x5557a0d68110_0;
    %cmpi/u 14, 0, 5;
    %jmp/0xz  T_1.10, 5;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x5557a0d56f20_0, 0, 2;
T_1.10 ;
T_1.9 ;
    %jmp T_1.7;
T_1.6 ;
    %load/vec4 v0x5557a0d56f20_0;
    %cmpi/e 0, 0, 2;
    %jmp/0xz  T_1.12, 4;
    %load/vec4 v0x5557a0d68110_0;
    %cmpi/u 12, 0, 5;
    %jmp/0xz  T_1.14, 5;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x5557a0d56f20_0, 0, 2;
    %jmp T_1.15;
T_1.14 ;
    %pushi/vec4 18, 0, 5;
    %load/vec4 v0x5557a0d68110_0;
    %cmp/u;
    %jmp/0xz  T_1.16, 5;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x5557a0d56f20_0, 0, 2;
T_1.16 ;
T_1.15 ;
T_1.12 ;
T_1.7 ;
T_1.1 ;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x5557a0d320f0;
T_2 ;
    %delay 2, 0;
    %pushi/vec4 0, 0, 5;
    %store/vec4 v0x5557a0d68250_0, 0, 5;
    %vpi_call 2 9 "$monitor", "Temperature =%d, state = %b", v0x5557a0d68250_0, v0x5557a0d68310_0 {0 0 0};
    %pushi/vec4 15, 0, 32;
T_2.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_2.1, 5;
    %jmp/1 T_2.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %vpi_func 2 11 "$random" 32 {0 0 0};
    %pad/s 5;
    %store/vec4 v0x5557a0d68250_0, 0, 5;
    %delay 15, 0;
    %jmp T_2.0;
T_2.1 ;
    %pop/vec4 1;
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "task3_tb.v";
    "./task3.v";
