#! /usr/bin/vvp
:ivl_version "10.3 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x560b677bc7e0 .scope module, "tb" "tb" 2 2;
 .timescale 0 0;
v0x560b677cd480_0 .var "A", 0 0;
v0x560b677cd550_0 .var "B", 0 0;
v0x560b677cd620_0 .net "C", 0 0, v0x560b677cd360_0;  1 drivers
S_0x560b677bc960 .scope module, "flipflop0" "flip_flop" 2 5, 3 1 0, S_0x560b677bc7e0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "A"
    .port_info 1 /INPUT 1 "B"
    .port_info 2 /OUTPUT 1 "C"
v0x560b67786070_0 .net "A", 0 0, v0x560b677cd480_0;  1 drivers
v0x560b677cd2a0_0 .net "B", 0 0, v0x560b677cd550_0;  1 drivers
v0x560b677cd360_0 .var "C", 0 0;
E_0x560b677bcda0 .event posedge, v0x560b677cd2a0_0, v0x560b67786070_0;
    .scope S_0x560b677bc960;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x560b677cd360_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x560b677bc960;
T_1 ;
    %wait E_0x560b677bcda0;
    %load/vec4 v0x560b67786070_0;
    %inv;
    %load/vec4 v0x560b677cd2a0_0;
    %inv;
    %and;
    %load/vec4 v0x560b677cd360_0;
    %and;
    %load/vec4 v0x560b67786070_0;
    %load/vec4 v0x560b677cd2a0_0;
    %inv;
    %and;
    %load/vec4 v0x560b677cd360_0;
    %inv;
    %and;
    %or;
    %load/vec4 v0x560b67786070_0;
    %load/vec4 v0x560b677cd2a0_0;
    %and;
    %or;
    %store/vec4 v0x560b677cd360_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x560b677bc7e0;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x560b677cd480_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x560b677cd550_0, 0, 1;
    %delay 2, 0;
    %vpi_call 2 10 "$monitor", "A = %b, B = %b, C = %b", v0x560b677cd480_0, v0x560b677cd550_0, v0x560b677cd620_0 {0 0 0};
    %pushi/vec4 20, 0, 32;
T_2.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_2.1, 5;
    %jmp/1 T_2.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %vpi_func 2 12 "$random" 32 {0 0 0};
    %pad/s 1;
    %store/vec4 v0x560b677cd480_0, 0, 1;
    %vpi_func 2 13 "$random" 32 {0 0 0};
    %pad/s 1;
    %store/vec4 v0x560b677cd550_0, 0, 1;
    %delay 15, 0;
    %jmp T_2.0;
T_2.1 ;
    %pop/vec4 1;
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "task2_tb.v";
    "./task2.v";
